<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.freshair.android.estadisticas"
    android:versionCode="6"
    android:versionName="1.3" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:testOnly="true" >
        <activity
            android:name="com.freshair.android.estadisticas.InitActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.freshair.android.estadisticas.MainActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.AltaChartActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.ItemChartManagerActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.AltaItemChartActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.ItemPerMonthActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.ListadoCompararChartsActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.ConfigChartActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.SimpleLinearChartActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.ComparisonChartActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.HelpActivity" >
        </activity>
        <activity android:name="com.freshair.android.estadisticas.AboutUsActivity" >
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
    </application>

</manifest>